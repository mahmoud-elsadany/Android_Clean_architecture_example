<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="bold_text_style">
        <item name="android:fontFamily">@font/sfprodisplay_bold</item>
    </style>

    <style name="regular_text_style">
        <item name="android:fontFamily">@font/sfprodisplay_regular</item>
    </style>

    <style name="semibold_text_style">
        <item name="android:fontFamily">@font/sfprodisplay_semibold</item>
    </style>





</resources>
